@model CodeAndCoffeeInfo.Core.Model.CCSession

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
	<h2>Sessions</h2>
	<ul>
	@*@foreach(var ccSession in Model) {*@
		<li>@Model.Name</li>
	@*  *@}
	</ul>
</div>

<form action="/" method="post">
	<div class="form-group">
		@Html.Label(m => m.Name).AddClass("col-md-2 control-label")
		<div class="col-md-10">
			@Html.Input(m => m.Name).AddClass("form-control")
		</div>
	</div>
	<div class="form-group">
		@Html.LabelFor(m => m.Name, new { @class = "col-md-2 control-label" })
		<div class="col-md-10">
			@Html.PasswordFor(m => m.Name, new { @class = "form-control" })
		</div>
	</div>
</form>